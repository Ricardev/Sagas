{
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://serviceDiscovery:8761/eureka/",
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": false
    },
    "Instance": {
      "NonSecurePort": 9000,
      "HostName": "Gateway",
      "InstanceId": "Gateway,Port:9000",
